   "source": [
    "x = (1,2,3,4,5,6,7,8,9)\n",
    "even = 0\n",
    "odd = 0\n",
    "for i in range(len(x)):\n",
    "    if x[i]%2==0:\n",
    "        even +=1\n",
    "    else:\n",
    "        odd +=1\n",
    "print('Number of Even Numbers = {} Number of Odd Numbers = {}'.format(even,odd))\n"
