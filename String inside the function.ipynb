   "source": [
    "def reverse_string(string):\n",
    "    reverse = ''\n",
    "    index = len(string)\n",
    "    while index > 0:\n",
    "        reverse += string[index-1]\n",
    "        index = index-1\n",
    "    return reverse"
   ]
  },
